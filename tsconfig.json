{
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": false,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "sourceMap": true,
    "noLib": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015", "dom", "es2016.array.include"]
  }
}
